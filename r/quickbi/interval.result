DROP TABLE IF EXISTS inter2;
CREATE TABLE inter2 (
id int,
name varchar(64),
age int,
amount DOUBLE,
address varchar(255),
birthday DATE,
create_time TIME,
update_time TIMESTAMP,
is_delete boolean,
PRIMARY KEY (id)
);
